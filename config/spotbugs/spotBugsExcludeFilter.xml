<?xml version="1.0" encoding="UTF-8"?>
  <FindBugsFilter
              xmlns="https://github.com/spotbugs/filter/3.0.0"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">
  <Match>
		<Or>
			<Package name="org.apache.commons.cli" />
            <Package name="org.apache.commons.io" />
            <Package name="org.apache.commons.io.file" />
            <Package name="org.apache.commons.io.function" />
            <Package name="org.apache.commons.io.input" />
            <Package name="org.apache.commons.io.output" />
            <Package name="org.apache.commons.lang3" />
            <Package name="org.apache.commons.lang3.builder" />
            <Package name="org.apache.commons.lang3.math" />
            <Package name="org.apache.commons.lang3.text" />
            <Package name="org.apache.commons.lang3.tuple" />
            <Package name="org.gradle.cli" />
            <Package name="org.gradle.wrapper" />
            <Class name="~org.jacoco.*" />
		</Or>
  </Match>
</FindBugsFilter>